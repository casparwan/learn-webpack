{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "edge": "17",
                    "firefox": "60",
                    "chrome": "67",
                    "safari": "11.1"
                }
            }
        ],
        [
            "@babel/preset-react"
        ]
    ],
    "plugins": [
        // "@babel/plugin-syntax-dynamic-import",
        
        ["@babel/plugin-proposal-class-properties"],
        ["@babel/plugin-transform-runtime",{
            "corejs": 3
        }],
        [
            "import",
            {
                "libraryName": "antd",
                "style": "css"
            }
        ]
    ]
}